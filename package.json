{
  "name": "react-on-rails",
  "version": "1.5.0",
  "description": "A demo in Rails 5 and React.js",
  "main": "server.js",
  "engines": {
    "node": "11.0.0",
    "yarn": "1.10.1"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently 'rails s' 'webpack-dev-server --hot --mode=development'",
    "build": "webpack -p --mode=production",
    "build-dev": "webpack -d --progress",
    "build-watch": "webpack-dev-server --hot --mode=development ",
    "clean": "rimraf public node_modules",
    "heroku-postbuild": "webpack -p && rimraf client webpack.config.js .babelrc .gitignore README.md"
  },
  "webpack_build": {
    "entry": "./client/src/main.jsx",
    "output": "public",
    "title": "demo react-on-rails",
    "react_mount_root": "container"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.generalassemb.ly/wdi-nyc/react-on-rails.git"
  },
  "author": "Jason Seminara",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://git.generalassemb.ly/wdi-nyc/react-on-rails/issues"
  },
  "homepage": "https://git.generalassemb.ly/wdi-nyc/react-on-rails#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "autoprefixer": "^9.1.5",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "bcrypt": "^3.0.1",
    "bloomer": "^0.6.5",
    "bulma": "^0.7.1",
    "clean-webpack-plugin": "^0.1.17",
